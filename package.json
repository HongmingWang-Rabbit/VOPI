{
  "name": "vopi-backend",
  "version": "2.0.0",
  "description": "VOPI - Video Object Processing Infrastructure backend service",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:worker": "tsx watch src/workers/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:worker": "node dist/workers/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "extract": "node src/smartFrameExtractor/index.js",
    "extract:help": "node src/smartFrameExtractor/index.js --help",
    "commercial": "node src/smartFrameExtractor/generateCommercial.js",
    "commercial:help": "node src/smartFrameExtractor/generateCommercial.js --help",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.500.0",
    "@aws-sdk/lib-storage": "^3.500.0",
    "@aws-sdk/s3-request-presigner": "^3.500.0",
    "@fastify/cors": "^9.0.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.0.0",
    "@google/generative-ai": "^0.21.0",
    "bullmq": "^5.1.0",
    "dotenv": "^16.4.0",
    "drizzle-orm": "^0.29.0",
    "fastify": "^4.25.0",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "ioredis": "^5.3.0",
    "pg": "^8.11.0",
    "pino": "^8.17.0",
    "pino-pretty": "^10.3.0",
    "sharp": "^0.33.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.0",
    "drizzle-kit": "^0.20.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "video",
    "frame-extraction",
    "product-photography",
    "gemini",
    "ai",
    "fastify",
    "microservice"
  ],
  "license": "MIT"
}
