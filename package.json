{
  "name": "vopi-backend",
  "version": "2.0.0",
  "description": "VOPI - Video Object Processing Infrastructure backend service",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:worker": "tsx watch src/workers/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:worker": "node dist/workers/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/db/seed-credits.ts",
    "db:seed:config": "tsx src/db/seed-global-config.ts",
    "extract": "node src/smartFrameExtractor/index.js",
    "extract:help": "node src/smartFrameExtractor/index.js --help",
    "commercial": "node src/smartFrameExtractor/generateCommercial.js",
    "commercial:help": "node src/smartFrameExtractor/generateCommercial.js --help",
    "keys": "tsx src/cli/api-keys.ts",
    "keys:create": "tsx src/cli/api-keys.ts create",
    "keys:list": "tsx src/cli/api-keys.ts list",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:cli": "tsx src/cli/test/index.ts",
    "test:endpoints": "tsx scripts/test-endpoints.ts",
    "test:oauth": "tsx scripts/test-oauth.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.500.0",
    "@aws-sdk/lib-storage": "^3.500.0",
    "@aws-sdk/s3-request-presigner": "^3.500.0",
    "@fastify/cors": "^9.0.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.0.0",
    "@google/generative-ai": "^0.21.0",
    "@inquirer/prompts": "^8.2.0",
    "@smithy/node-http-handler": "^4.4.8",
    "amazon-sp-api": "^1.2.0",
    "bullmq": "^5.1.0",
    "chalk": "^5.6.2",
    "dotenv": "^16.4.0",
    "drizzle-orm": "^0.29.0",
    "fastify": "^4.25.0",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "ioredis": "^5.3.0",
    "jose": "^6.1.3",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.11.0",
    "pino": "^8.17.0",
    "pino-pretty": "^10.3.0",
    "sharp": "^0.33.0",
    "stripe": "^20.2.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitest/coverage-v8": "^4.0.17",
    "drizzle-kit": "^0.20.0",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.17"
  },
  "keywords": [
    "video",
    "frame-extraction",
    "product-photography",
    "gemini",
    "ai",
    "fastify",
    "microservice"
  ],
  "license": "MIT",
  "pnpm": {
    "patchedDependencies": {
      "amazon-sp-api@1.2.0": "patches/amazon-sp-api@1.2.0.patch"
    }
  }
}
